{ 
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "func-names": 0,
    "eol-last": 0,
    "prefer-template": 0,
    "new-cap": 0,
    /* TO REMOVE */
    "quote-props": 0,
    "quotes": 0
  },
  "plugins": [
    "react",
  ]
}